@import "/pages/components/base/base.wxss";

page {
  background-color: #fff;
}

.container {
  background-color: #fff;
  min-height: 100%;
}

.cus-warp {
  text-align: center;
  border: 0;
  border-radius: 0;
  background-color: transparent;
}

.cus-warp::after {
  content: "";
  border: 0;
  border-radius: 0;
}

.cus-icon {
  width: 710rpx;
  height: 82rpx;
  vertical-align: bottom;
  margin: 50rpx auto;
}

.list {
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}

.item {
  border: 2rpx solid #ffa827;
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.item-icon-warp {
  width: 100rpx;
  height: 100rpx;
  background-color: #ffa827;
  display: flex;
  align-items: center;
  justify-content: center;
}

.item-icon {
  width: 64rpx;
  height: 64rpx;
  vertical-align: bottom;
}

.item-info {
  width: calc(100% - 256rpx);
  padding-left: 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #1e1e1e;
}

.item-des {
  width: 156rpx;
  padding-right: 12rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  font-weight: bold;
  color: #ffa827;
}

.footer-zuanla{
  background-color: #fff;
}