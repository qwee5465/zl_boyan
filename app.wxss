/* 弹窗遮罩通用样式 */
.popup{position: fixed;top:0;left:0;width:100%;height:100%;z-index:99;display: flex;justify-content: center;align-items: center;}
.mask{position: fixed;top:0;left:0;width:100%;height:100%;background-color:rgba(0,0,0,.5);z-index:100;}

