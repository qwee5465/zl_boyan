.btn-yellow {color: #2E2E2E !important;background-color: #ffc900 !important;}

/* 阻止滚动条滚动 */
.container.over-hidden {position: fixed;top: 0;left: 0;right: 0;bottom: 0;height: 100%;width: 100%;z-index: 1;}
/*弹窗遮罩通用样式  */
.t-popup{position: fixed;top:0;left:0;width:100%;height:100%;z-index:99;display: flex;justify-content: center;align-items: center;}
.t-mask{position: fixed;top:0;left:0;width:100%;height:100%;background-color:rgba(0,0,0,.5);z-index:100;}
.not-data {text-align: center;padding:20rpx 0;font-size: 24rpx;color: #aeaeae;}

/* 富文本通用 */
.wxParse-p{padding:0 20rpx;box-sizing: border-box;}
.wxParse-img{max-width: 100%;}

/* 占位线 */
.line{height: 10rpx;background-color:#f2f2f2; }

/* radio通用样式 */
.radio-box {display: inline-block;width: 40rpx;height: 40rpx;border: 1px solid #acacac;border-radius: 50%;box-sizing: border-box;margin-right: 24rpx;}
.radio-checked-icon {width: 40rpx;height: 40rpx;display: none;}
.radio-box.active {border: none;}
.radio-box.active .radio-checked-icon {display: block;}

/* 数量加减样式 */
.compute-warp {height: 28px;}
.compute-minus {position: relative;display: inline-block;width: 28px;height: 28px;color: #666;background-color: #ededed;border-radius: 8rpx;}
.compute-minus::after {content: "";position: absolute;top: 13px;left: 9px;width: 10px;height: 2px;background-color: #666;}
.compute-minus.disabled::after,.compute-plus.disabled::after,.compute-plus.disabled::before {background-color: #ccc;}
.compute-text {display: inline-block;width: 35px;height: 28px;font-size: 24rpx;color: #2e2e2e;text-align: center;vertical-align: top;box-sizing: border-box;}
.compute-plus {position: relative;display: inline-block;width: 28px;height: 28px;color: #666;background-color: #ededed;border-radius: 8rpx;}
.compute-plus::after {content: "";position: absolute;top: 13px;left: 9px;width: 10px;height: 2px;background-color: #666;}
.compute-plus::before {content: "";position: absolute;top: 9px;left: 13px;width: 2px;height: 10px;background-color: #666;}
